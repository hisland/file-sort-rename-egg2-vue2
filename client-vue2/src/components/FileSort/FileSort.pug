layoutUpDown
  .topArea(slot="up")
    .sepWrap
      .aLink(
        v-for="(vv1, index1) in pathSepList"
        @click="changePath(vv1)"
        :path="vv1.url"
        :key="index1") {{vv1.name}}/
    .btns
      .btn 保存
  .fileList(slot="middle")
    .head
      .item
        span.col1
        span.col2: input(type="checkbox" ng-model="check_all" ng-change="checkAll()")
        span.col3 原名
        span.col4 新名
    .body
      .item(v-for="(vv1, index1) in fileList" :key="index1")
        span.col1
        span.col2: input(type="checkbox" ng-model="check_all" ng-change="checkAll()")
        span.col3
          .aLink(v-if="vv1.url") {{vv1.name}}
          span(v-else) {{vv1.name}}
        span.col4 新名
